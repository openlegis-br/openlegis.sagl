<dtml-comment>
connection_id : dbcon_sagl
arguments : cod_materia
max_rows : 1000
max_cache : 100
</dtml-comment>
SELECT DISTINCT cod_materia

FROM tramitacao

<dtml-sqlgroup where>
  tramitacao.ind_excluido = 0
  AND NOT (tramitacao.ind_ult_tramitacao = 0 AND tramitacao.dat_encaminha IS NULL)

<dtml-and>
  <dtml-sqltest cod_materia column="tramitacao.cod_materia" op="=" type="int" optional>

</dtml-sqlgroup>
