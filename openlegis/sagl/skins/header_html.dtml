        <title><dtml-if expr="REQUEST.get('firstHeading', '')"><dtml-var "REQUEST.get('firstHeading', '')"> — </dtml-if>SAGL</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta content="Gestão de Processos Digitais" name="description" />
        <meta content="OpenLegis" name="author" />
        <!-- Skip links para acessibilidade -->
        <style>
            .skip-link {
                position: absolute;
                top: -40px;
                left: 0;
                background: #000;
                color: #fff;
                padding: 8px;
                text-decoration: none;
                z-index: 100;
            }
            .skip-link:focus {
                top: 0;
            }
        </style>
        <!-- App favicon -->
        <link rel="shortcut icon" href="<dtml-var portal_url>/imagens/favicon.ico">
        <link rel="manifest" href="<dtml-var portal_url>/manifest.json">
